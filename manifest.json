{
   "manifest_version": 3,
 
   "name": "CodeBuddy Extension",
   "description": "CodyBuddy 팀에서 제작한 Chrome Extension 입니다",
   "version": "1.0",
 
   "content_scripts": [
       {
           "matches": ["https://ide.dev.goorm.io/workspace/*", "<all_urls>"],
           "js": ["dist/contentScript.js"]
       }
   ],
 
   "background": {
     "service_worker": "dist/background.js"
   },
 
   "permissions": [
      "storage", "activeTab", "scripting", "tabs"
   ],
 
   "host_permissions": [
     "<all_urls>"
   ]
}